<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.*?>
<AnchorPane id="AnchorPane" prefHeight="250.0" prefWidth="500.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
    <children>
        <BorderPane prefHeight="250.0" prefWidth="500.0" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
            <left>
                <ListView fx:id="list" prefHeight="200.0" prefWidth="180.0" BorderPane.alignment="CENTER" />
            </left>
            <center>
                <Button mnemonicParsing="false" onAction="#Copy" text="Copy&gt;&gt;&gt;" BorderPane.alignment="CENTER" />
            </center>
            <right>
                <TextArea fx:id="area" prefHeight="200.0" prefWidth="180.0" BorderPane.alignment="CENTER" />
            </right>
        </BorderPane>
    </children>
</AnchorPane>
